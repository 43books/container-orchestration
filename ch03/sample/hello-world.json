{
  "id": "hello-world",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "dockercloud/hello-world",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 80, "hostPort": 0, "protocol": "tcp"}
      ],
      "privileged": false,
      "parameters": [
        { "key": "memory", "value": "32m" }
      ]
    }
  },
  "cpus": 0.1,
  "mem": 16.0,
  "disk": 0.0,
  "maxLaunchDelaySeconds": 600,
  "instances": 1
}
